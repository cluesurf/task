{
  "name": "@termsurf/task",
  "version": "0.1.70",
  "main": "./host/code/node.js",
  "browser": "./host/code/browser.js",
  "type": "module",
  "chocoVersion": "0.0.2",
  "bin": {
    "task": "./host/code/cli/index.js"
  },
  "scripts": {
    "test": "./test/cli.sh",
    "bump:choco": "pnpm tsx bump.choco",
    "bind:dock": "source .env && echo $GITHUB_TOKEN | docker login ghcr.io -u $GITHUB_USERNAME --password-stdin",
    "make:dock": "source .env && docker build --progress plain --platform linux/amd64 . -t termsurf/task && pnpm mark:dock",
    "make:dock:show": "source .env && docker build --progress plain --platform linux/amd64 . -t termsurf/task && pnpm mark:dock",
    "host:dock": "pnpm mark:dock && source .env && docker push ghcr.io/termsurf/task:latest",
    "mark:dock": "source .env && docker tag termsurf/task:latest ghcr.io/termsurf/task:latest",
    "rise:dock": "source .env && docker run --platform linux/amd64 --name task -d -i -t termsurf/task bash",
    "call:dock": "docker exec -d task",
    "hook:dock": "docker exec -it task bash",
    "kill:dock": "docker kill task",
    "toss:dock": "docker rm task",
    "size:dock": "source ./hook.sh && print_bytes $(docker inspect -f \"{{ .Size }}\" termsurf/task)",
    "make": "tsc && tsc-alias",
    "move": "pnpm make && git add . && git commit -am 'save' && npm version patch && pnpm host && git push",
    "scan": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "host": "rm -rf host && mkdir host && find code -name '*.wasm' | cpio -pdm host ; pnpm make && pnpm make:puppeteer && npm publish --access=public",
    "make:puppeteer": "webpack build",
    "scan:puppeteer": "webpack watch"
  },
